a=int(input('enter the first number: '))
b=int(input('enter the number: '))
c=input('enter what do you want to do "+,-,/,*":')
while c ()
if c=='+':
    print(a+b)
# elif a==56 and b==5 and c=='+':
#     print("77")
# elif a==45 and b==3 and c=='*':
    print("555")
elif c== '-':
    print(a-b)
elif c=='*':
    print(a*b)
elif c=='/':
    print(a/b)
else:
    print('go away')
